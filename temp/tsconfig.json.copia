{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["ES6", "DOM"],
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "typeRoots": ["../node_modules/@types"]
  },
  "include": ["**/*.ts"],
  "exclude": ["/assets"]
}
