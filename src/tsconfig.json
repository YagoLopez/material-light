{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "typeRoots": ["../node_modules/@types"]
  },
  "exclude": ["/node_modules/, /assets, /temp"],
  "include": ["**/*.ts"]
}
